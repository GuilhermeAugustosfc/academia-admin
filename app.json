{
  "expo": {
    "name": "academia-admin",
    "slug": "academia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "googleServicesFile": "./google-services.json",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": ["./local/assets/notification_sound.wav"]
        }
      ]
    ],
    "splash": {
      "image": "./local/assets/notification-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ministrodamagia123.academia"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "dcec87bf-2510-4b85-aa05-90821f91c9cf"
      }
    },
    "owner": "ministrodamagia123"
  }
}
